{
  "extends": "nyc-config-tsx",
  "all": true,
  "exclude": [".next", "babel.config.js", "tests/*.spec.ts"],
  "reporter": ["html", "text", "lcov"],
  "report-dir": "./coverage",
  "check-coverage": true,
  "include": ["src/**/*.tsx", "src/**/*.ts"],
  "extension": [".ts", ".tsx"],
  "sourceMap": true,
  "instrument": true
}
